@model IEnumerable<AztecAppleDepApp.Models.SearchResultViewModel>

@{
    ViewBag.Title = "Search Result";
}

<h2>Search Result</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.RecId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DepTransactionDate)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DepTransactionType)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DepTransactoinNote)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.RecId)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DepTransactionDate)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DepTransactionType)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DepTransactoinNote)
        </td>
        <td>
            @Html.ActionLink("Details", "Details", new { Id = item.RecId })
        </td>
    </tr>
}

</table>
