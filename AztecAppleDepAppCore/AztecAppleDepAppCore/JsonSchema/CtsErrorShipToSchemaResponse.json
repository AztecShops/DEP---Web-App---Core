{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/root.json",
  "type": "object",
  "title": "The Root Schema",
  "required": [
    "errorCode",
    "errorMessage",
    "transactionId"
  ],
  "properties": {
    "errorCode": {
      "$id": "#/properties/errorCode",
      "type": "string",
      "title": "The Errorcode Schema",
      "default": "",
      "examples": [
        "GRX-50025"
      ],
      "pattern": "^(.*)$"
    },
    "errorMessage": {
      "$id": "#/properties/errorMessage",
      "type": "string",
      "title": "The Errormessage Schema",
      "default": "",
      "examples": [
        "Ship-To entered is not valid. Please enter a valid Ship-To"
      ],
      "pattern": "^(.*)$"
    },
    "transactionId": {
      "$id": "#/properties/transactionId",
      "type": "string",
      "title": "The Transactionid Schema",
      "default": "",
      "examples": [
        "13836d68-7353-4acc-a813-0e217be8f04d-1424990915792"
      ],
      "pattern": "^(.*)$"
    }
  }
}