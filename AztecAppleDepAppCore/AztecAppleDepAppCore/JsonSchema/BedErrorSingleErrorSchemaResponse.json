{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/root.json",
  "type": "object",
  "title": "The Root Schema",
  "required": [
    "enrollDeviceErrorResponse"
  ],
  "properties": {
    "enrollDeviceErrorResponse": {
      "$id": "#/properties/enrollDeviceErrorResponse",
      "type": "object",
      "title": "The Enrolldeviceerrorresponse Schema",
      "required": [
        "errorCode",
        "errorMessage"
      ],
      "properties": {
        "errorCode": {
          "$id": "#/properties/enrollDeviceErrorResponse/properties/errorCode",
          "type": "string",
          "title": "The Errorcode Schema",
          "default": "",
          "examples": [
            "GRX-1056"
          ],
          "pattern": "^(.*)$"
        },
        "errorMessage": {
          "$id": "#/properties/enrollDeviceErrorResponse/properties/errorMessage",
          "type": "string",
          "title": "The Errormessage Schema",
          "default": "",
          "examples": [
            "DEP Reseller ID missing. Enter a valid DEP Reseller ID and resubmit your request."
          ],
          "pattern": "^(.*)$"
        }
      }
    }
  }
}